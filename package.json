{
  "name": "event-management-system",
  "private": true,
  "scripts": {
    "backend": "mvn -f \"event-management-backend\" spring-boot:run",
    "frontend": "npm.cmd --prefix \"event-management-frontend\" start",
    "start": "concurrently -k -n backend,frontend -c green,cyan \"npm run backend\" \"npm run frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
